{
  "name": "katerina",
  "description": "Моя страна Беларусь имеет множество прекрасных, удивительных и уникальных достопримечательностей. Мое приложение собрало лишь некоторые из них и расскажет вам как до них добраться.",
  "version": "1.0.0",
  "author": "Max Eremin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ermaxnet/katerina.git"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "helmet": "^3.12.1",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.22.2",
    "mongoose": "^5.1.5",
    "nodemailer": "^4.6.7",
    "normalize.css": "^8.0.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pug": "^2.0.3",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-scripts": "1.1.4",
    "serve-static": "^1.13.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./node_modules/ src/ -o src/",
    "build-js": "react-scripts build",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules/ src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "jest --config ./jest.config.json",
    "eject": "react-scripts eject",
    "start-admin": "node ./katerina-admin/index.js"
  },
  "devDependencies": {
    "bourbon": "^5.0.1",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "image-webpack-loader": "^4.3.1",
    "jest": "^23.1.0",
    "mais-mongoose-seeder": "^1.0.7",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "supertest": "^3.1.0",
    "susy": "^3.0.5"
  }
}
